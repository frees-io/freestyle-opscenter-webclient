{
  "name": "freestyle-opscenter",
  "version": "0.0.2",
  "author": "Juan Valencia Calvellido <juanvalencia@calvellido.es>",
  "license": "MIT",
  "keywords": [
    "freestyle",
    "opscenter",
    "microservices",
    "websockets",
    "angular"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --build-optimizer --aot",
    "serve": "ng serve",
    "serve:prod": "ng serve --prod --build-optimizer --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "protofetch:custom": "build-scripts/fetch-proto.js",
    "protofetch:metrics": "build-scripts/fetch-proto.js -s http://localhost:8080/proto/models/metrics",
    "protofetch:microservices": "build-scripts/fetch-proto.js -s http://localhost:8080/proto/models/microservices",
    "protofetch": "npm run protofetch:metrics --silent && npm run protofetch:microservices --silent",
    "protodef:custom": "build-scripts/gen-proto-code.js",
    "protodef": "build-scripts/gen-proto-code.js -i proto -o src/app/shared/proto",
    "mock-server:install": "cd mock-server && npm install",
    "mock-server": "cd mock-server && npm start"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.2",
    "@angular/cdk": "^5.0.0-rc.1",
    "@angular/common": "^5.0.2",
    "@angular/compiler": "^5.0.2",
    "@angular/core": "^5.0.2",
    "@angular/forms": "^5.0.2",
    "@angular/http": "^5.0.2",
    "@angular/material": "^5.0.0-rc.1",
    "@angular/platform-browser": "^5.0.2",
    "@angular/platform-browser-dynamic": "^5.0.2",
    "@angular/platform-server": "^5.0.2",
    "@angular/router": "^5.0.2",
    "@swimlane/ngx-charts": "^7.0.1",
    "core-js": "^2.5.1",
    "d3": "^4.11.0",
    "lato-font": "^3.0.0",
    "material-design-icons": "^3.0.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.3",
    "@angular/compiler-cli": "^5.0.2",
    "@angular/language-service": "^5.0.2",
    "@types/jasmine": "^2.8.2",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^8.0.53",
    "axios": "^0.17.1",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.7.2",
    "ora": "^1.3.0",
    "protractor": "~5.1.2",
    "shelljs": "^0.7.8",
    "ts-node": "~3.2.0",
    "ts-protoc-gen": "^0.4.0",
    "tslint": "~5.3.2",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "2.4.2",
    "yargs": "^10.0.3"
  }
}
